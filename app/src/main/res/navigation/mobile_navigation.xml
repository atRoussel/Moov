<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/navigation_schedule"
    tools:ignore="UnusedNavigation">

    <fragment
        android:id="@+id/navigation_schedule"
        android:name="fr.epf.moov.fragment.ScheduleFragment"
        android:label="Moov"
        tools:layout="@layout/fragment_schedule" />

    <fragment
        android:id="@+id/navigation_qrcode"
        android:name="fr.epf.moov.fragment.QrCodeFragment"
        android:label="QR Code"
        tools:layout="@layout/fragment_qrcode"/>

    <fragment
        android:id="@+id/navigation_list"
        android:name="fr.epf.moov.fragment.ListFragment"
        android:label="Transport"
        tools:layout="@layout/fragment_lists"/>

    <fragment
        android:id="@+id/navigation_saved"
        android:name="fr.epf.moov.fragment.FavorisFragment"
        android:label="Favoris"
        tools:layout="@layout/fragment_favoris"/>
    <fragment
        android:id="@+id/navigation_map"
        android:name="fr.epf.moov.fragment.MapsFragment"
        android:label="Cartes"
        tools:layout="@layout/fragment_maps"/>

</navigation>