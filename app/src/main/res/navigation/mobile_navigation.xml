<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/navigation_home"
    tools:ignore="UnusedNavigation">

    <fragment
        android:id="@+id/navigation_home"
        android:name="fr.epf.moov.fragment.ScheduleFragment"
        android:label="fragment_schedule"
        tools:layout="@layout/fragment_schedule" />

    <fragment
        android:id="@+id/navigation_find"
        android:name="fr.epf.moov.fragment.QrCodeFragment"
        android:label="fragment_qrcode"
        tools:layout="@layout/fragment_qrcode"/>

    <fragment
        android:id="@+id/navigation_profil"
        android:name="fr.epf.moov.fragment.ListFragment"
        android:label="fragment_lists"
        tools:layout="@layout/fragment_lists"/>

    <fragment
        android:id="@+id/navigation_saved"
        android:name="fr.epf.moov.fragment.FavorisFragment"
        android:label="fragment_favoris"
        tools:layout="@layout/fragment_favoris"/>
    <fragment
        android:id="@+id/navigation_trip"
        android:name="fr.epf.moov.fragment.MapsFragment"
        android:label="fragment_maps"
        tools:layout="@layout/fragment_maps"/>

</navigation>